      <portrait>Portraits/grinch</portrait>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/CultOfCthulhu.CosmicEntityDef[defName="Entity_Dagon"]/titles</xpath>
    <value>
      <titles>The Christmas Thief, Anti-Who, The Demon of Envy, 1 of Hope</titles>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/CultOfCthulhu.CosmicEntityDef[defName="Entity_Dagon"]/domains</xpath>
