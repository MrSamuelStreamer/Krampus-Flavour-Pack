      <portrait>Portraits/hansgruber</portrait>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/CultOfCthulhu.CosmicEntityDef[defName="Entity_Hastur"]/titles</xpath>
    <value>
      <titles>The Fallen One, The Mastermind</titles>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/CultOfCthulhu.CosmicEntityDef[defName="Entity_Hastur"]/domains</xpath>
