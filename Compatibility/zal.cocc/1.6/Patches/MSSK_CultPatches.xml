<?xml version="1.0" encoding="utf-8"?>
<Patch> 
  <!-- Shub to Krampus -->
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/CultOfCthulhu.CosmicEntityDef[defName="Entity_Shub"]/label</xpath>
    <value>
      <label>Krampus</label>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/CultOfCthulhu.CosmicEntityDef[defName="Entity_Shub"]/portrait</xpath>
    <value>
      <portrait>Portraits/krampus</portrait>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/CultOfCthulhu.CosmicEntityDef[defName="Entity_Shub"]/titles</xpath>
    <value>
      <titles>The Punisher, The Black Goat, Shadow of Claus, Père Fouettard, The Swatter, The Child-Snatcher</titles>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/CultOfCthulhu.CosmicEntityDef[defName="Entity_Shub"]/domains</xpath>
    <value>
      <domains>Punishment, Retribution, Justice</domains>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/CultOfCthulhu.CosmicEntityDef[defName="Entity_Shub"]/description</xpath>
    <value>
      <description>The bringer of punishments.</description>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/CultOfCthulhu.CosmicEntityDef[defName="Entity_Shub"]/descriptionLong</xpath>
    <value>
      <descriptionLong>The great horned one - Krampus appears as a large black-furred, goat-like figure, who accompanies the gift-bringer. Unlike his travel companion, Krampus brings only just deserts to those who disobey their betters, and scoff in the the face of authority. The justice inflicted by Krampus ranges from cruel gifts to physical torment to, for the worst offenders of all, being carried to Krampus's realm of eternal darkness.</descriptionLong>
    </value>
  </Operation>

  <!-- Dagon to the Grinch -->
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/CultOfCthulhu.CosmicEntityDef[defName="Entity_Dagon"]/label</xpath>
    <value>
      <label>The Grinch</label>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/CultOfCthulhu.CosmicEntityDef[defName="Entity_Dagon"]/portrait</xpath>
