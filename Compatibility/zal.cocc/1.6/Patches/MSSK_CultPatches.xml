      <portrait>Portraits/abominablesnowman</portrait>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/CultOfCthulhu.CosmicEntityDef[defName="Entity_Bast"]/titles</xpath>
    <value>
      <titles>The Ice Giant, Yeti, Snowstalker</titles>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/CultOfCthulhu.CosmicEntityDef[defName="Entity_Bast"]/domains</xpath>
