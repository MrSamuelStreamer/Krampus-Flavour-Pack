      <domains>Control, Domination, Manipulation</domains>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/CultOfCthulhu.CosmicEntityDef[defName="Entity_Hastur"]/description</xpath>
